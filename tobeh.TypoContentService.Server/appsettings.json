{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "TypoImageGen": "Trace"
    }
  },
  "AllowedHosts": "*",
  "ImageGenerator": {
    "CachePath": "cache",
    "ByteChunkKByte": 64
  },
  "Grpc": {
    "HostPort": 8080,
    "ValmarAddress": "GRPC_VALMAR_URL_SED_PLACEHOLDER"
  },
  "S3": {
    "AccessKey": "S3_ACCESS_KEY_SED_PLACEHOLDER",
    "SecretKey": "S3_SECRET_KEY_SED_PLACEHOLDER"
  },
  "Git": {
    "RepositoryUrl": "GIT_REPOSITORY_URL_SED_PLACEHOLDER",
    "AccessToken": "GIT_ACCESS_TOKEN_SED_PLACEHOLDER",
    "Path": "git"
  }
}
